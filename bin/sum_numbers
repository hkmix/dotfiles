#!/usr/bin/env python

import fileinput

array = []
count = 0.0

for line in fileinput.input():
    array.extend(map(lambda x: int(x), line.strip().split(" ")))

print("Nums:  " + str(array))
print("Count: " + str(len(array)))
print("Total: " + str(sum(array)))
print("Mean:  " + str(float(sum(array)) / len(array)))
