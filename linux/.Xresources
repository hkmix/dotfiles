URxvt.font: xft:iosevka slab:regular:pixelsize=18,xft:opensymbol
URxvt.boldFont: xft:iosevka slab:bold:pixelsize=18
URxvt.italicFont: xft:iosevka slab:italic:pixelsize=18
URxvt.bolditalicFont: xft:iosevka slab:bold:italic:pixelsize=18
URxvt.scrollBar: false
URxvt.internalBorder: 8

Xft.antialias: true
Xft.rgba:      none
Xft.hinting:   true
Xft.hintstyle: hintslight
Xft.dpi:       96

Xcursor.size:  32

rofi.color-enabled: true
rofi.color-window: #002b36, #93a1a1, #003642
